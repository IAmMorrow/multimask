{
    "name": "Multimask",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Use all your favorite dapps directly from Ledger Live.",
    "homepage_url": "https://ledger.com",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_title": "TSRWPCX",
      "default_popup": "popup.html"
    },
    "default_locale": "en",
    "background": {
      "scripts": [
        "js/background.js"
      ],
      "persistent": true
    },
    "permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": ["*.ttf" ],
    "content_security_policy": "connect-src 'self'; default-src 'self'; style-src 'self' https://fonts.googleapis.com data: 'unsafe-inline'; font-src https://fonts.gstatic.com data: 'unsafe-inline'",
    "content_scripts": [
      {
        "matches": [
          "https://*/*"
        ],
        "js": [
          "js/content.js"
        ]
      }
    ]
  }
  
